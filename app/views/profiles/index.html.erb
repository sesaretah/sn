<div class="row ">
  <div class="col-lg-12">
    <div class="card">
      <div class="card-header" style="background-color: #fff;">
        <div class="input-group">
          <input type="text" class="form-control" placeholder="<%=t :search%>">
          <div class="input-group-append">
            <button type="button" class="btn btn-secondary">
              <i class="fe fe-search"></i>
            </button>
          </div>
        </div>
      </div>
      <div class="card-body" style="background: #f5f7fb;">
        <div id='profiles'>
          <%= render partial:'/profiles/list', locals: {profiles: @profiles}%>
        </div>
        <%= render partial: '/profiles/more', locals:{page: @page}%>
      </div>
    </div>
  </div>
</div>
